# !/bin/sh
# Aliases that have been programmatically added.
# Touch at your own risk.
alias c='cd'
alias cscope='cscope -bR'
alias ctags-c++='ctags -R --c++-kinds=+p --fields=+iaS --extra=+q'
alias df='df -h'
alias du='du -h'
alias e='emacs'
alias g='git'
alias ga='git add'
alias gb='git branch'
alias gc='git checkout'
alias gcb='git checkout -b'
alias gd='git diff'
alias gdl='gd --color=always | less'
alias gh='git help'
alias gi='git commit'
alias gia='git commit -a'
alias gl='git log'
alias gm='git merge'
alias gpl='git pull'
alias gplr='git pull --rebase'
alias gps='git push'
alias gpsf='git push --force'
alias gr='git rebase'
alias gra='git rebase --abort'
alias grc='git rebase --continue'
alias gs='git status'
alias gsh='git stash'
alias gsm='m git submodule'
alias gt='git tag'
alias k='kill'
alias l='ls'
alias m='mv'
alias maketex="make -f ${HOME}/.latex/Makefile"
alias mdiff='diff -ubE'
alias mdu='du -d 1 -h'
alias mfe='free -m'
alias mfn='find . -name'
alias mg='rgrep'
alias mls='ls -lhFA'
alias mrm='rm -rf'
alias mtop='top -o cpu'
alias p='cp'
alias ping='ping -c4'
alias rm='rm -i'
